			<xsd:documentation>UF (
                                        AC - Acre;
                                        AL - Alagoas;
                                        AM - Amazonas;
                                        AP - Amapa;
                                        BA - Bahia;
                                        CE - Ceara;
                                        DF - Distrito Federal;
                                        ES - Espirito Santo;
                                        GO - Goias;
                                        MA - Maranhao;
                                        MG - Minas Gerais;
                                        MS - Mato Grosso do Sul;
                                        MT - Mato Grosso;
                                        PA - Para;
                                        PB - Paraiba;
                                        PE - Pernambuco;
                                        PI - Piaui;
                                        PR - Parana;
                                        RJ - Rio de Janeiro;
                                        RN - Rio Grande do Norte;
                                        RO - Rondonia;
                                        RR - Roraima;
                                        RS - Rio Grande do Sul;
